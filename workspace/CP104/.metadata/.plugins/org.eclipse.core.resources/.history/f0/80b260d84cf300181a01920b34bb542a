alphabet = "abcdefghijklmnopqrstuvwxyz"
vignereTable = [[char for char in alphabet] for char2 in alphabet]
for line in vignereTable:
    print(line)
    
filename = "mp4in"
fh = open(filename, "r")

key = input("Enter key: ")

encode(fh, key)