alphabet = "abcdefghijklmnopqrstuvwxyz"
vignereTable = [[char for char in alphabet] for char2 in alphabet]
for line in vignereTable:
    print(line)
    
filename = "mp4in"
fh = open(filename, "r")

key = input("Enter key: ")

def encode(fh, key):
    """
    Encodes a file by a Vignere Table given a key
    """
    fh.seek(0)
    file_contents = fh.readlines()
    
    for line in file_contents:
        for char in line:
            pass
    return

encode(fh, key)